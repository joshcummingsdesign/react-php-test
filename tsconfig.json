{
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "lib": ["esnext", "dom"],
        "jsx": "react-jsx",
        "moduleResolution": "node",
        "allowJs": true,
        "strict": false,
        "sourceMap": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "baseUrl": "./",
        "paths": {
            "@/*": ["src/*"]
        }
    },
    "include": ["./src", "./globals.d.ts"]
}
